    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "jest-css-modules": "^2.0.0",
    "nodemon": "^1.18.11",
    "prettier": "^1.16.4",
    "react-addons-test-utils": "^15.6.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
